                always {
                    //junit 'target/surefire-reports/*.xml'
                    echo 'junit target/surefire-reports/*.xml'
                    
                }
            }
        }
        stage('Deliver') {
            steps {
                //sh './jenkins/scripts/deliver.sh'
                echo './jenkins/scripts/deliver.sh'
            }
        }
    }
}
