        stage('Build') {
            steps {
                //sh 'mvn -B -DskipTests clean package'
                echo 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test') {
            steps {
                //sh 'mvn test'
                echo 'mvn test'
            }
